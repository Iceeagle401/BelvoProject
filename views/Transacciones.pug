link(rel="stylesheet" href="css/style-transactions.css")
title transactions
a(href='/HomePage') Home
div
 form(action="/Transacciones" method="post")
  input(type="date" id="fromDate" name="fromDate" value=fromDate min="2018-01-01" max=today)
  input(type="date" id="toDate" name="toDate" value=toDate min="2018-01-01"  max=today)
  button(type="submit") Filtrar
for transaction in transactions
 div
  hidden 
  h2=transaction.merchant
  li=transaction.amount
  li=transaction.currency
  li=transaction.createdAt
  li=transaction.reference
 
          